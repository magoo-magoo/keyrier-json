{
    "name": "@keyrier/ui",
    "version": "1.1.35",
    "private": true,
    "description": "Keyrier JSON",
    "homepage": "./",
    "repository": {
        "type": "git",
        "url": "https://github.com/magoo-magoo/keyrier-json"
    },
    "license": "MIT",
    "author": "magoo",
    "main": "public/electron.js",
    "scripts": {
        "analyze": "yarn build && source-map-explorer --html 'build/static/**/*.*' > source-map-explorer.html",
        "build": "react-scripts build --profile",
        "build:ci": "npm run-script version:ci && npm run-script build",
        "cypress:open": "cypress open",
        "cypress:run": "concurrently --success first --kill-others \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && cypress run\"",
        "predeploy": "yarn build",
        "deploy": "gh-pages -d build",
        "eject": "react-scripts eject",
        "electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
        "postinstall": "electron-builder install-app-deps",
        "lh": "lighthousebot --runner=wpt",
        "pack:dockerelectron": "bash -c \"docker run --rm --env GH_TOKEN=$GITHUB_TOKEN --env-file <(env | grep -iE 'DEBUG|NODE_|ELECTRON_|YARN_|NPM_|CI|CIRCLE|TRAVIS|APPVEYOR_|CSC_|_TOKEN|_KEY|AWS_|STRIP|BUILD_') -v ${PWD}:/project -v ~/.cache/electron:/root/.cache/electron -v ~/.cache/electron-builder:/root/.cache/electron-builder electronuserland/builder:wine /bin/bash -c \\\"npm run pack:electron\\\"\"",
        "pack:electron": "electron-builder --linux AppImage --windows nsis --publish never",
        "start": "react-scripts start",
        "test": "react-scripts test",
        "stryker:run": "stryker run"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}",
            "!src/**/*.d.ts",
            "!src/registerServiceWorker.ts",
            "!<rootDir>/node_modules/",
            "!<rootDir>/path/to/dir/"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 40,
                "lines": 40,
                "statements": 40
            }
        },
        "moduleNameMapper": {
            "ace-builds": "<rootDir>/../node_modules/ace-builds"
        },
        "transformIgnorePatterns": [
            "/!node_modules\\/lodash-es/"
        ],
        "coverageReporters": [
            "text",
            "clover",
            "json",
            "lcov"
        ]
    },
    "dependencies": {
        "@keyrier/core": "^1.1.35",
        "@reduxjs/toolkit": "^1.4.0",
        "bootswatch": "4.5.0",
        "chevrotain": "7.0.1",
        "classnames": "2.2.6",
        "electron-log": "4.2.2",
        "electron-updater": "4.3.1",
        "fast-deep-equal": "3.1.3",
        "localforage": "1.7.4",
        "lodash-es": "4.17.15",
        "match-sorter": "4.1.0",
        "perfume.js": "5.0.2",
        "react": "16.13.1",
        "react-ace": "9.0.0",
        "react-debounce-input": "3.2.2",
        "react-dom": "16.13.1",
        "react-grid-layout": "0.18.3",
        "react-json-view": "1.19.1",
        "react-redux": "7.2.0",
        "react-select": "3.1.0",
        "react-table": "7.2.0",
        "react-toastify": "6.0.6",
        "react-use": "15.2.2",
        "reactstrap": "8.5.0",
        "redux": "4.0.5",
        "redux-logger": "^3.0.6",
        "redux-undo": "1.0.1",
        "utility-types": "3.10.0",
        "xlsx": "0.16.2"
    },
    "devDependencies": {
        "@stryker-mutator/core": "3.2.4",
        "@stryker-mutator/jest-runner": "3.2.4",
        "@stryker-mutator/typescript": "3.2.4",
        "@testing-library/react": "10.2.1",
        "@types/classnames": "2.2.10",
        "@types/jest": "^26.0.3",
        "@types/lodash-es": "4.17.3",
        "@types/match-sorter": "4.0.0",
        "@types/node": "^14.0.14",
        "@types/react": "16.9.38",
        "@types/react-dom": "16.9.8",
        "@types/react-grid-layout": "0.17.1",
        "@types/react-loadable": "5.5.3",
        "@types/react-redux": "7.1.9",
        "@types/react-select": "3.0.13",
        "@types/react-table": "7.0.19",
        "@types/reactstrap": "8.4.3",
        "@types/redux-logger": "^3.0.8",
        "@types/scheduler": "0.16.1",
        "cypress": "4.8.0",
        "electron": "8.3.1",
        "electron-builder": "22.7.0",
        "eslint": "^7.5.0",
        "eslint-config-prettier": "^6.11.0",
        "gh-pages": "2.2.0",
        "jest": "26.1.0",
        "jest-environment-jsdom-sixteen": "1.0.3",
        "lighthousebot": "https://github.com/GoogleChromeLabs/lighthousebot",
        "node-sass": "4.14.1",
        "prettier": "^2.0.5",
        "react": "^17.0.0-rc.0",
        "react-dom": "^17.0.0-rc.0",
        "react-scripts": "next",
        "redux-devtools": "3.5.0",
        "typescript": "^3.9.5",
        "unfetch": "4.1.0"
    },
    "build": {
        "appId": "com.github.magoo.keyrier.json",
        "productName": "Keyrier JSON",
        "copyright": "Copyright Â© ${author}",
        "publish": [
            "github"
        ],
        "linux": {
            "maintainer": "mael.gouacide@gmail.com",
            "executableName": "keyrier-json",
            "icon": "build/logo_512.png"
        },
        "win": {
            "icon": "build/logo_512.png"
        }
    },
    "gitHead": "ca6cf54996f96a78aca4ed57a92c760778ec1fc4",
    "readme": "./README.md"
}
