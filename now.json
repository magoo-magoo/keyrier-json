{
    "builds": [
        {
            "src": "src/api/**/*.ts",
            "use": "@now/node"
        }
    ],
    "routes": [
        { "src": "/custom-page", "headers": { "cache-control": "s-maxage=1000" }, "dest": "/index.html" },
        { "src": "/api/ping", "dest": "src/api/ping.ts" },
        { "src": "/api/run", "dest": "src/api/run.ts" },
        { "src": "/", "status": 301, "headers": { "Location": "https://keyrier.magoo.dev" } }
    ]
}
