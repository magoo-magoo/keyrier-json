{
    "name": "keyrier-json",
    "private": true,
    "description": "Query JSON objects with SQL syntax",
    "repository": "git@github.com:magoo-magoo/keyrier-json.git",
    "license": "MIT",
    "author": "magoo <contact@magoo.dev>",
    "workspaces": [
        "cli",
        "core",
        "ui"
    ],
    "scripts": {
