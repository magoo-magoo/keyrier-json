{
  "name": "keyrier-json",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "bootstrap": "4.1.3",
    "bootswatch": "4.1.3",
    "classnames": "2.2.6",
    "jquery": "3.3.1",
    "lodash": "4.17.11",
    "popper.js": "1.14.4",
    "react": "16.6.0",
    "react-ace": "6.2.0",
    "react-dom": "16.6.0",
    "react-loadable": "5.5.0",
    "react-redux": "5.1.0",
    "react-select": "2.1.0",
    "react-table": "6.8.6",
    "reactstrap": "6.5.0",
    "redux": "4.0.1",
    "xlsx": "0.14.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "devDependencies": {
    "@types/react-loadable": "5.4.1",
    "@types/classnames": "2.2.6",
    "@types/enzyme": "3.1.14",
    "@types/enzyme-adapter-react-16": "1.0.3",
    "@types/jest": "23.3.9",
    "@types/lodash": "4.14.117",
    "@types/node": "10.12.1",
    "@types/prop-types": "15.5.6",
    "@types/react": "16.4.18",
    "@types/react-dom": "16.0.9",
    "@types/react-redux": "6.0.9",
    "@types/react-select": "2.0.5",
    "@types/react-table": "6.7.14",
    "@types/reactstrap": "6.4.2",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.6.0",
    "gh-pages": "2.0.1",
    "react-scripts": "2.1.1",
    "react-test-renderer": "16.6.0",
    "redux-devtools": "3.4.1",
    "typescript": "3.1.5"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/data/*.{ts,tsx}",
      "!src/registerServiceWorker.ts",
      "!/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 40,
        "lines": 40,
        "statements": 40
      }
    },
    "coverageReporters": [
      "json",
      "lcov",
      "text"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
