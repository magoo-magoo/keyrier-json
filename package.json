        "@types/jest": "25.2.3",
        "@types/node": "12.12.45",
        "concurrently": "5.2.0",
        "eslint-config-prettier": "6.11.0",
        "husky": "4.2.5",
        "jest": "24.9.0",
        "lerna": "3.22.0",
        "prettier": "2.0.5",
        "pretty-quick": "2.0.1",
        "source-map-explorer": "2.4.2",
        "ts-jest": "24.3.0",
        "tsdx": "0.13.2",
        "typescript": "3.9.5",
        "wait-on": "4.0.2"
