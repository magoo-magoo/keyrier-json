{
  "description": "Keyrier JSON",
  "name": "keyrier-json",
  "author": "magoo",
  "version": "0.5.3",
  "private": true,
  "homepage": "./",
  "repository": {
    "type": "git",
    "url": "https://github.com/magoo-magoo/keyrier-json"
  },
  "main": "public/electron.js",
  "dependencies": {
    "bootstrap": "4.1.3",
    "bootswatch": "4.1.3",
    "classnames": "2.2.6",
    "electron-log": "^2.2.17",
    "electron-updater": "^3.1.6",
    "jquery": "3.3.1",
    "lodash": "4.17.11",
    "popper.js": "1.14.4",
    "react": "16.7.0-alpha.0",
    "react-ace": "6.2.0",
    "react-dom": "16.7.0-alpha.0",
    "react-loadable": "5.5.0",
    "react-redux": "5.1.0",
    "react-select": "2.1.1",
    "react-table": "6.8.6",
    "reactstrap": "6.5.0",
    "redux": "4.0.1",
    "xlsx": "0.14.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build",
    "pack:electron": "electron-builder --linux AppImage --windows nsis --publish never",
    "pack:dockerelectron": "bash -c \"docker run --rm --env GH_TOKEN=$GITHUB_TOKEN --env-file <(env | grep -iE 'DEBUG|NODE_|ELECTRON_|YARN_|NPM_|CI|CIRCLE|TRAVIS|APPVEYOR_|CSC_|_TOKEN|_KEY|AWS_|STRIP|BUILD_') -v ${PWD}:/project -v ~/.cache/electron:/root/.cache/electron -v ~/.cache/electron-builder:/root/.cache/electron-builder electronuserland/builder:wine /bin/bash -c \\\"npm run pack:electron\\\"\"",
    "postinstall": "electron-builder install-app-deps",
    "electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "devDependencies": {
    "@types/classnames": "2.2.6",
    "@types/enzyme": "3.1.15",
    "@types/enzyme-adapter-react-16": "1.0.3",
    "@types/jest": "23.3.9",
    "@types/lodash": "4.14.117",
    "@types/node": "10.12.1",
    "@types/prop-types": "15.5.6",
    "@types/react": "16.4.18",
    "@types/react-dom": "16.0.9",
    "@types/react-loadable": "5.4.1",
    "@types/react-redux": "6.0.9",
    "@types/react-select": "2.0.6",
    "@types/react-table": "6.7.15",
    "@types/reactstrap": "6.4.2",
    "concurrently": "^4.0.1",
    "electron": "^3.0.7",
    "electron-builder": "^20.31.2",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.6.0",
    "gh-pages": "2.0.1",
    "husky": "^1.1.3",
    "prettier": "1.14.3",
    "pretty-quick": "^1.8.0",
    "react-scripts": "2.1.1",
    "react-test-renderer": "16.6.0",
    "redux-devtools": "3.4.1",
    "typescript": "3.1.5",
    "wait-on": "^3.1.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/data/*.{ts,tsx}",
      "!src/registerServiceWorker.ts",
      "!/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 40,
        "lines": 40,
        "statements": 40
      }
    },
    "coverageReporters": [
      "json",
      "lcov",
      "text"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "build": {
    "appId": "com.github.magoo.keyrier.json",
    "productName": "Keyrier JSON",
    "copyright": "Copyright Â© ${author}",
    "publish": [
      "github"
    ],
    "linux": {
      "maintainer": "mael.gouacide@gmail.com",
      "executableName": "keyrier-json",
      "icon": "build/logo_512.png"
    },
    "win": {
      "icon": "build/logo_512.png"
    }
  }
}
