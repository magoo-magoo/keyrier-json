{
  "description": "Keyrier JSON",
  "name": "keyrier-json",
  "author": "magoo",
  "version": "0.5.64",
  "private": true,
  "homepage": "./",
  "repository": {
    "type": "git",
    "url": "https://github.com/magoo-magoo/keyrier-json"
  },
  "main": "public/electron.js",
  "dependencies": {
    "bootstrap": "4.3.1",
    "bootswatch": "4.2.1",
    "classnames": "2.2.6",
    "electron-log": "3.0.1",
    "electron-updater": "4.0.6",
    "jquery": "3.3.1",
    "lodash": "4.17.11",
    "popper.js": "1.14.7",
    "react": "16.8.2",
    "react-ace": "6.4.0",
    "react-debounce-input": "3.2.0",
    "react-dom": "16.8.2",
    "react-json-view": "1.19.1",
    "react-loadable": "5.5.0",
    "react-redux": "6.0.1",
    "react-select": "2.3.0",
    "react-table": "6.9.0",
    "react-toastify": "4.5.2",
    "reactstrap": "7.1.0",
    "redux": "4.0.1",
    "sql-parser": "0.5.0",
    "utility-types": "3.4.1",
    "xlsx": "0.14.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "yarn build",
    "lint": "prettier --config .prettierrc 'src/**/*.{js,ts,tsx}' --write && tslint -p . --format codeFrame --fix",
    "deploy": "gh-pages -d build",
    "pack:electron": "electron-builder --linux AppImage --windows nsis --publish never",
    "pack:dockerelectron": "bash -c \"docker run --rm --env GH_TOKEN=$GITHUB_TOKEN --env-file <(env | grep -iE 'DEBUG|NODE_|ELECTRON_|YARN_|NPM_|CI|CIRCLE|TRAVIS|APPVEYOR_|CSC_|_TOKEN|_KEY|AWS_|STRIP|BUILD_') -v ${PWD}:/project -v ~/.cache/electron:/root/.cache/electron -v ~/.cache/electron-builder:/root/.cache/electron-builder electronuserland/builder:wine /bin/bash -c \\\"npm run pack:electron\\\"\"",
    "postinstall": "electron-builder install-app-deps",
    "electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "devDependencies": {
    "@types/classnames": "2.2.7",
    "@types/enzyme": "3.1.17",
    "@types/enzyme-adapter-react-16": "1.0.3",
    "@types/jest": "24.0.2",
    "@types/lodash": "4.14.120",
    "@types/node": "10.12.25",
    "@types/prop-types": "15.5.8",
    "@types/react": "16.8.2",
    "@types/react-dom": "16.8.0",
    "@types/react-loadable": "5.5.0",
    "@types/react-redux": "7.0.1",
    "@types/react-select": "2.0.11",
    "@types/react-table": "6.7.21",
    "@types/react-test-renderer": "16.8.0",
    "@types/reactstrap": "7.1.3",
    "@types/redux-mock-store": "1.0.0",
    "concurrently": "4.1.0",
    "electron": "4.0.4",
    "electron-builder": "20.38.5",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.9.1",
    "gh-pages": "2.0.1",
    "husky": "1.3.1",
    "node-sass": "4.11.0",
    "prettier": "1.16.4",
    "pretty-quick": "1.10.0",
    "react-scripts": "2.1.5",
    "react-test-renderer": "16.8.2",
    "react-testing-library": "5.8.0",
    "redux-devtools": "3.5.0",
    "redux-mock-store": "1.5.3",
    "tslint": "5.12.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-react": "3.6.0",
    "typescript": "3.3.3",
    "wait-on": "3.2.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/data/*.{ts,tsx}",
      "!src/registerServiceWorker.ts",
      "!/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 40,
        "lines": 40,
        "statements": 40
      }
    },
    "coverageReporters": [
      "json",
      "lcov",
      "text"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,css,md}": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ]
  },
  "build": {
    "appId": "com.github.magoo.keyrier.json",
    "productName": "Keyrier JSON",
    "copyright": "Copyright Â© ${author}",
    "publish": [
      "github"
    ],
    "linux": {
      "maintainer": "mael.gouacide@gmail.com",
      "executableName": "keyrier-json",
      "icon": "build/logo_512.png"
    },
    "win": {
      "icon": "build/logo_512.png"
    }
  }
}
