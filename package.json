{
  "description": "Keyrier JSON",
  "name": "keyrier-json",
  "author": "magoo",
  "version": "0.5.131",
  "private": true,
  "homepage": "./",
  "repository": {
    "type": "git",
    "url": "https://github.com/magoo-magoo/keyrier-json"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "version:ci": "npm version patch --force -m \"release v%s [skip ci]\"",
    "build:ci": "npm run-script version:ci && npm run-script build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "yarn build",
    "cypress:open": "cypress open",
    "cypress:run": "concurrently --success first --kill-others \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && cypress run\"",
    "lint": "prettier --config .prettierrc 'src/**/*.{js,jsx,ts,tsx}' --write && tslint -p . --format codeFrame --fix",
    "deploy": "gh-pages -d build",
    "pack:electron": "electron-builder --linux AppImage --windows nsis --publish never",
    "pack:dockerelectron": "bash -c \"docker run --rm --env GH_TOKEN=$GITHUB_TOKEN --env-file <(env | grep -iE 'DEBUG|NODE_|ELECTRON_|YARN_|NPM_|CI|CIRCLE|TRAVIS|APPVEYOR_|CSC_|_TOKEN|_KEY|AWS_|STRIP|BUILD_') -v ${PWD}:/project -v ~/.cache/electron:/root/.cache/electron -v ~/.cache/electron-builder:/root/.cache/electron-builder electronuserland/builder:wine /bin/bash -c \\\"npm run pack:electron\\\"\"",
    "postinstall": "electron-builder install-app-deps",
    "electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "analyze": "yarn build && source-map-explorer --html 'build/static/**/*.*' > source-map-explorer.html"
  },
  "dependencies": {
    "bootswatch": "4.3.1",
    "chevrotain": "5.0.0",
    "classnames": "2.2.6",
    "electron-log": "3.0.7",
    "electron-updater": "4.1.2",
    "fast-deep-equal": "2.0.1",
    "lodash": "4.17.15",
    "react": "16.9.0",
    "react-ace": "7.0.2",
    "react-debounce-input": "3.2.0",
    "react-dom": "16.9.0",
    "react-json-view": "1.19.1",
    "react-redux": "7.1.0",
    "react-select": "3.0.4",
    "react-table": "6.10.0",
    "react-toastify": "5.3.2",
    "reactstrap": "8.0.1",
    "redux": "4.0.4",
    "redux-undo": "1.0.0-beta9-9-7",
    "sql-parser": "0.5.0",
    "utility-types": "3.7.0",
    "xlsx": "0.15.0"
  },
  "devDependencies": {
    "@types/classnames": "2.2.7",
    "@types/jest": "24.0.17",
    "@types/lodash": "4.14.123",
    "@types/node": "10.14.15",
    "@types/prop-types": "15.7.0",
    "@types/react": "16.8.13",
    "@types/react-dom": "16.8.4",
    "@types/react-loadable": "5.5.1",
    "@types/react-redux": "7.0.6",
    "@types/react-select": "2.0.19",
    "@types/react-table": "6.8.1",
    "@types/reactstrap": "8.0.0",
    "@types/scheduler": "0.12.1",
    "concurrently": "4.1.1",
    "cypress": "3.4.1",
    "electron": "6.0.1",
    "electron-builder": "21.2.0",
    "gh-pages": "2.1.1",
    "husky": "3.0.3",
    "node-sass": "4.12.0",
    "prettier": "1.18.2",
    "pretty-quick": "1.11.1",
    "react-scripts": "3.0.1",
    "react-testing-library": "8.0.1",
    "redux-devtools": "3.5.0",
    "source-map-explorer": "2.0.1",
    "tslint": "5.18.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-react": "4.0.0",
    "typescript": "3.5.3",
    "unfetch": "4.1.0",
    "wait-on": "3.3.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts}",
      "!/src/Components/*.{tsx}",
      "!src/data/*.{ts,tsx}",
      "!src/registerServiceWorker.ts",
      "!/node_modules/"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text"
    ]
  },
  "coverageThreshold": {
    "global": {
      "branches": 40,
      "lines": 40,
      "statements": 40
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,css,md}": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ]
  },
  "main": "public/electron.js",
  "build": {
    "appId": "com.github.magoo.keyrier.json",
    "productName": "Keyrier JSON",
    "copyright": "Copyright Â© ${author}",
    "publish": [
      "github"
    ],
    "linux": {
      "maintainer": "mael.gouacide@gmail.com",
      "executableName": "keyrier-json",
      "icon": "build/logo_512.png"
    },
    "win": {
      "icon": "build/logo_512.png"
    }
  }
}
